model Settings {
  id                  String  @id @default(cuid())
  name                String
  email               String  @unique
  phone               String?
  picture             String?
  governingBody       String?
  governingBodyNumber String?
  streetAddress       String?
  city                String?
  county              String?
  postTown            String?
  postCode            String?
  country             String? @default("United Kingdom")
}
